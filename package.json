    "base-64": "^1.0.0",
    "expo": "~52.0.25",
    "expo-image-picker": "~16.0.4",
    "expo-secure-store": "^14.0.1",
    "expo-status-bar": "~2.0.1",
    "lz-string": "^1.5.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.6",
    "react-native-aria": "^0.2.3",
